        INCLUDES = -I$(NEMOINC)
 LOCAL_LIBRARIES = -lX11 $(L)
   SYS_LIBRARIES = -lm
	    SRCS = yapp_x.c X.c Xutil.c font.c testyapp.c event.c
	    OBJS = yapp_x.o X.o Xutil.o font.o testyapp.o event.o

 PROGRAM = yappx

all:: yappx

yappx: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

saber_yappx:: $(SRCS)
	# load $(ALLDEFINES) $(SRCS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

osaber_yappx:: $(OBJS)
	# load $(ALLDEFINES) $(OBJS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

install:: yappx
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  yappx $(DESTDIR)$(BINDIR)

install.man:: yappx.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) yappx.man $(DESTDIR)$(MANDIR)/yappx.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

all:: libyapp_x.a

libyapp_x.a:  yapp_x.o X.o Xutil.o font.o event.o
	$(RM) $@
	$(AR) $@ `lorder  yapp_x.o X.o Xutil.o font.o event.o | tsort`
	$(RANLIB) $@

