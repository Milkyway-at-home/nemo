#! /bin/csh -f
#
# a quick hack   - Nov 2010
#
#  need:  libboost, libboost-system, libboost-thread
#
# g77:        -lfrtbegin -lg2c
# gfortran:   -lgfortran

set dir=$NEMO/usr/jcl/uns_project

# for linux
set args=""


if (-e $dir) then
  cd $dir/build
  cmake .
  make
  make install 
else
  echo mknemo.d: $dir does not exist
endif

